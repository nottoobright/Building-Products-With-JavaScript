{
    "extends": "prettier",
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "max-len": ["error", 120, 4]
    }
}